numTypes = 2
twojmax =     8
rcutfac = 4.812302818
rfac0 = 0.99363
rmin0 = 0.0
wj1 = 1.0
wj2 = 0.9590493408
zblcutinner = 4.0
zblcutouter = 4.8
qcoul = 1.e-8
rcoul = 10.0
radelem1 = 0.5
radelem2 = 0.417932464
type1 = W
type2 = Be
zblz1 = 74
